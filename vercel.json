{
  "version": 2,
  "rewrites": [
    { "source": "/api/auth/complete-profile", "destination": "/api/auth" },
    { "source": "/api/auth/complete-cuidador-profile", "destination": "/api/auth" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/Public/index.html" },
    { "src": "/(.*)", "dest": "/Public/$1" }
  ]
}
